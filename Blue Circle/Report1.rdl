  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Username">
            <Value>=Parameters!Username.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 'Bill' Datatype,Mb.Cli_Name,mb.Yr,mb.PM_Username,mb.PM_Name,SUM(case when apb.yr = Year(DATEADD(yy, DATEDIFF(yy, 0, dateadd(year,-3,getdate())), 0)) then yr3 end) as yr3,
SUM(case when apb.yr = Year(DATEADD(yy, DATEDIFF(yy, 0, dateadd(year,-2,getdate())), 0)) then yr2 end) as yr2,SUM(case when apb.yr = Year(DATEADD(yy, DATEDIFF(yy, 0, dateadd(year,-1,getdate())), 0)) then yr1 end) as yr1,SUM(case when apb.yr = Year(DATEADD(yy, DATEDIFF(yy, 0, dateadd(year,-1,getdate())), 0)) then Inv_Amt end) yrnow,mb.Prj_idno,mb.Prj_Name,mb.MS_BDCode,MS_Name,mb.Emp_Initials MSL_Initials,pn.projected,pn.Notes,'' init_name,0 projected_rev,0 projected_bud
from Monthly_Billings mb
left outer join
Ajera_Projection_Billings apb
on mb.Cli_Name = apb.cli_name and mb.PM_Username = apb.pm_username and mb.Yr = apb.yr
left outer join
Projection_Notes pn
on mb.Cli_Name = pn.cli_name and mb.PM_Username = pn.pm_username and pn.projected_yr = Year(dateadd(year,1,getdate()))
where mb.PM_Username in (@Username)
Group By Mb.Cli_Name,mb.Yr,mb.PM_Username,mb.PM_Name,mb.Prj_idno,mb.Prj_Name,mb.MS_BDCode,MS_Name,mb.Emp_Initials,projected,Notes
Union All
select 'Bill' Datatype,pnc.cli_name,yr,pnc.pm_username,pm.Emp_LastName + ', ' + pm.Emp_FirstName + ' ' + pm.Emp_MiddleInitial + '.' pm_name,0 yr3,0 yr2,0 yr1,0 yrnow,'' Prj_idno,'' Prj_Name,ms.MS_BDCode,MS_Name,msl.Emp_Initials MSL_Initials,pn.projected,pn.Notes,'' init_name,0 projected_rev,0 projected_bud
from Projection_New_Clients pnc 
inner join Gateway_ODS.dbo.Employee pm
on pnc.pm_username = pm.Emp_CompanyUserName
inner join Gateway_ODS.dbo.MarketSegment ms
on pnc.MS_ID = ms.MS_ID
Left Outer Join Projection_Notes pn
on pnc.cli_name = pn.cli_name and pnc.pm_username = pn.pm_username and pn.projected_yr = Year(dateadd(year,1,getdate()))
inner join Gateway_ODS.dbo.Employee msl
on ms.Emp_Key = msl.Emp_Key
where pm.Emp_CompanyUserName in (@Username)</CommandText>
      </Query>
